@model IEnumerable<Abantu_System.Models.ProgrammeMember>

@{
    ViewBag.Title = "ProgrammeMembers";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2></h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Programme ID")
        </th>
        <th>
            @Html.DisplayName("Programme Name")
        </th>
        <th>
            @Html.DisplayName("Member ID")
        </th>
        <th>
            @Html.DisplayName("First Name")
        </th>
        <th>
            @Html.DisplayName("Last Name")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Programme_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Programme.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member_ID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.Surname)
        </td>
    </tr>
}

</table>
